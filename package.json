{
  "name": "@joaomelo/libt",
  "version": "0.1.31",
  "description": "Libt is an optioned JavaScript library template. The code itself has no practical utility; it serves as a placeholder to be replaced when using the template.",
  "main": "lib/dist/index.js",
  "scripts": {
    "start": "npm run dev:lib",
    "dev:lib": "jest --watch --coverage=false",
    "dev:demo": "webpack-dev-server --config demo/config/webpack.config.js",
    "lint": "eslint */src/**/*.js",
    "test": "npm run lint && jest",
    "build": "rimraf lib/dist/* && webpack --config lib/config/webpack.config.js",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joaomelo/libt.git"
  },
  "keywords": [
    "template",
    "library",
    "npm"
  ],
  "author": "jo√£o melo <j@melo.plus> (https://blog.melo.plus)",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/joaomelo/libt/issues"
  },
  "homepage": "https://github.com/joaomelo/libt#readme",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.1.0",
    "circular-dependency-plugin": "^5.2.0",
    "css-loader": "^3.4.2",
    "dotenv-webpack": "^1.7.0",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "rimraf": "^3.0.2",
    "style-loader": "^1.1.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  }
}
